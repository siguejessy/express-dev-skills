<%- include('../partials/header') -%>

<h3>Dev Skills as of <%= time %></h3>
  <% skills.forEach(function(s) { %>
  <li>
    <a href="/skills/<%= s.id %>"><%= s.skill %></a>
    - <%= s.done ? 'done' : 'not done' %>
    <form
    action="/skills/<%= s.id %>?_method=DELETE"
    method="POST"
    class="delete-form"
    ><button type="submit">🗑️</button>
  </form> | 
</li>
  <br>
  <% }); %>
  <br>
<a href="/skills/new">Add a skill</a>
<%- include('../partials/footer') -%>
